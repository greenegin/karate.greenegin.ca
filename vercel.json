{
  "routes": [
    {
      "src": "/manifest.webmanifest",
      "dest": "/manifest.webmanifest"
    }
  ],
  "headers": [
    {
      "source": "/manifest.webmanifest",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        }
      ]
    }
  ]
}
