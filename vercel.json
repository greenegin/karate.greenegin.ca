{
  "buildCommand": "npm run build",
  "devCommand": "npm run dev",
  "installCommand": "npm install",
  "framework": null,
  "functions": {
    "build/server/index.js": {
      "runtime": "edge",
      "maxDuration": 60
    }
  },
  "headers": [
    {
      "source": "/build/client/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/build/server/index.js"
    },
    {
      "source": "/(.*)",
      "destination": "/build/server/index.js"
    }
  ]
}